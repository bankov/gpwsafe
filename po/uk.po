#: src/main.cc:62
msgid "libgcrypt version mismatch"
msgstr "Не співпадає версія libgcrypt"

#: src/main.cc:73
msgid "Can't initialize libgcrypt"
msgstr "Не вдалося ініціалізувати libgcrypt"

#: src/StdoutEmitter.cc:35
#, c-format
msgid "Going to print %s to stdout"
msgstr "Буде надруковано %s у стандартний вивід"

#: src/App.cc:86
msgid "Commands"
msgstr "Команди"

#: src/App.cc:95
msgid "create an empty database"
msgstr "створити порожню базу даних"

#: src/App.cc:104
msgid ""
"list all entries matching [REGEX]. If either -u or -p are given, only one "
"entry may match. If neither -u or -p are given and one entry matches, print "
"details."
msgstr ""
"перерахувати всі записи, які відповідають [REGEX]. Якщо задано одне з -u чи -p, має бути знайдено "
"єдиний запис. Якщо не задано жодне з -u чи -p, і знайдено єдиний запис, роздрукувати "
"подробиці."

#: src/App.cc:115
msgid "add an entry"
msgstr "додати запис"

#: src/App.cc:124
msgid "edit an entry"
msgstr "редагувати запис"

#: src/App.cc:127
msgid "Options"
msgstr "Опції"

#: src/App.cc:131
msgid "specify the database file"
msgstr "зазначити файл бази даних"

#: src/App.cc:134
msgid "emit username of listed account"
msgstr "видати назву користувача"

#: src/App.cc:137
msgid "emit password of listed account"
msgstr "видати пароль"

#: src/App.cc:145
msgid "force echoing of entry to stdout"
msgstr "примусово надрукувати у стандартний вивід"

#: src/App.cc:155
msgid "force copying of entry to X selection"
msgstr "примусово скопіювати у буфер обміну Х"

#: src/App.cc:159
msgid "don't press on the pool of randomness"
msgstr "не виснажувати збірник випадковості"

#: src/App.cc:160
msgid "display this help and exit"
msgstr "показати цю допомогу і вийти"

#: src/App.cc:161
msgid "output version information and exit"
msgstr "вивести відомості про версію і вийти"

#: src/App.cc:180
msgid ""
" - command line tool compatible with Conterpane's PasswordSafe\n"
"\n"
msgstr ""
" — інструмент командного рядка, сумісний з Conterpane PasswordSafe\n"
"\n"

#: src/App.cc:182
msgid "Usage: "
msgstr "Вживання: "

#: src/App.cc:182
msgid " [OPTION] command [ARG]\n"
msgstr " [OPTION] команда [ARG]\n"

#: src/App.cc:198
msgid "Ambiguous command"
msgstr "Неоднозначна команда"

#: src/App.cc:219
msgid "Exception: "
msgstr "Виняток: "
